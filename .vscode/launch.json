{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run test",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/testing/main.go"
    },
    {
      "name": "Run command assume",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/assume/main.go"
    },
    {
      "name": "Run uninstall",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/granted/main.go",
      "args": ["uninstall"]
    },
    {
      "name": "Run command granted",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/granted/main.go",
      "args": ["completion"],
      "env": { "FORCE_NO_ALIAS": "true" }
    },
    {
      "name": "Run command assume active role",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/assume/main.go",
      "args": ["test-role", "-c", "-s iam", "-r region-name"],
      "env": {
        "FORCE_NO_ALIAS": "true",
        "AWS_PROFILE": "cf-dev"
      }
    },
    {
      "name": "Run command assume cf-dev",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/assume/main.go",
      "args": ["cf-dev"],
      "env": {
        "FORCE_NO_ALIAS": "true"
      }
    },
    {
      "name": "Remove token",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/granted/main.go",
      "args": [
        "token",
        "remove",
        "https://d-976708da7d.awsapps.com/start---(test)"
      ],
      "env": {
        "FORCE_NO_ALIAS": true
      }
    },
    {
      "name": "Run command url",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/assume/main.go",
      "args": ["cf-dev", "-c"],
      "env": {
        "FORCE_NO_ALIAS": "true"
      }
    },
    {
      "name": "Run command exec",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/assume/main.go",
      "args": ["cf-testing", "--exec", "aws s3 ls"],
      "env": {
        "FORCE_NO_ALIAS": "true",
        "AWS_PROFILE": "demo-sandbox1"
      }
    }
  ]
}
