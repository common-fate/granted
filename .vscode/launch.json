  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run command assume",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/assume/main.go",
      "args": ["-c", "cf-dev", "-r=hello"],
      "env": { "FORCE_NO_ALIAS": true }
    },
    {
      "name": "Run command assume active role",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cmd/assume/main.go",
      "args": ["test-role", "-c", "-s iam", "-r region-name"],
      "env": {
        "FORCE_NO_ALIAS": true,
        "GRANTED_AWS_ROLE_PROFILE": "cf-dev"
      }
    }
  ]
}
